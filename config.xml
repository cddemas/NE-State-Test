<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap		= "http://phonegap.com/ns/1.0"
	id				= "com.phonegap.northeaststate"
	versionCode		= "93"
	version 		= "1.5.0">

	<name>NE State 2</name>

	<description>
		Northeast State Community College Phone App
	</description>

	<author href="http://www.northeaststate.edu/">
		Russell Bowman
	</author>

	<gap:platforms>
		<gap:platform name="android" minVersion="1.5" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>
	
	<gap:splash src="splash/Default.png" />	
	
	<icon src="icons/icon.png" width="57" height="57" gap:role="default" />
	<icon src="icons/ic_launcher_android_48.png"  />
	<icon src="icons/ic_launcher_android_72.png"  />
		
	<preference name="orientation" value="default" />
	<preference name="target-device" value="universal" />
	
	<feature name="http://plugins.phonegap.com/ChildBrowser/2.0.0" /> 
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>

	<!-- sample preference specification -->
	<!-- <preference name="autorotate" value="false" readonly="true"/> -->
</widget>
